<mxStylesheet as="stylesheet">
    <add as="defaultVertex">
        <add as="shape" value="rectangle"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="align" value="left"/>
        <add as="verticalAlign" value="middle"/>
        <add as="fontColor" value="#000000"/>
        <add as="fontSize" value="11"/>
        <add as="fontStyle" value="0"/>
        <add as="spacingLeft" value="4"/>
        <add as="imageWidth" value="48"/>
        <add as="imageHeight" value="48"/>
        <add as="strokeWidth" value="2"/>
        <add as="strokeColor" value="#1B78C8"/>
        <add as="fillColor" value="#FFFFFF"/>
    </add>
    <add as="table">
        <add as="shape" value="swimlane"/>
        <add as="perimeter" value="rectanglePerimeter"/>
        <add as="align" value="left"/>
        <add as="gradientColor" value="#41B9F5"/>
        <add as="fillColor" value="#8CCDF5"/>
        <add as="swimlaneFillColor" value="#FFFFFF"/>
        <add as="fontColor" value="#000000"/>
        <add as="startSize" value="28"/>
        <add as="veticalAlign" value="middle"/>
        <add as="fontSize" value="12"/>
        <add as="image" value="../images/icons48/table.png"/>
        <add as="shadow" value="1"/>
        <add as="spacingLeft" value="48"/>
    </add>
    <add as="defaultEdge">
        <add as="align" value="center"/>
        <add as="fontColor" value="#446299"/>
        <add as="shape" value="connector"/>
        <add as="strokeColor" value="#6482B9"/>
        <add as="verticalAlign" value="middle"/>
        <add as="labelBackgroundColor" value="#FFFFFF"/>
        <add as="rounded" value="1"/>
        <add as="strokeWidth" value="2"/>
        <add as="startSize" value="8"/>
        <add as="endSize" value="8"/>
        <add as="startArrow" value="solo_uno"/>
        <add as="endArrow" value="solo_uno"/>
    </add>
</mxStyleSheet>